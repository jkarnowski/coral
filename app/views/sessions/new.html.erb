<body class="containerr">
<% provide(:title, "Login") %>
	<div class="boxing">
    <br>
  <h1 class="loginfish">Login</h1>
  	<div class="row">
  		<div class="col-md-6 col-md-offset-3">
       <%= form_for :sessions, :url => url_for(controller: '/sessions', action: 'create'), :html => {:method => :post} do |f| %>

          <%= f.label :email %>
          <%= f.email_field :email, class: 'form-control' %>
          <br>
          <%= f.label :password %>
          <%= f.password_field :password, class: 'form-control' %>

          <br>

          <%= f.submit "Log in", class: "btn btn-primary" %>

         <% end %>
      </div>
    </div>
  <br>
  </div>
</body>

